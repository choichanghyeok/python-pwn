풀이


import sys

input = sys.stdin.readline

test_case = int(input())

dp = [0 for i in range(101)]    # 전체개수

def w():
    dp[1],dp[2],dp[3] = 1,1,1    # 초깃값 할당
    dp[4],dp[5] = 2,2
    dp[6] = 3

    for i in range(7,101):
        dp[i] = dp[i-1] + dp[i-5]   #  점화식
    return

for _ in range(test_case):
    n = int(input())
    w()
    print(dp[n])


#점화식 : P[n-1] + P[n-5] (n>5)
